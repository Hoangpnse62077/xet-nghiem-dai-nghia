<script>
    var availableTags = getData("");
    $("#ddlPatientChoose").autocomplete({
        source: availableTags
    });

    $('#ddlPatientChoose').off('change').on('change', function () {
        var searchKey = $(this).val();

        $("#ddlPatientChoose").autocomplete({
            source: getData(searchKey)
        });
    });

    $(document).ready(function () {

        $.each(patients, function (i, item) {
            $('#ddlPatients')
                .append($("<option></option>")
                    .attr("value", item.PatientId)
                    .attr("data-subtext", item.HomeAddress)
                    .text(item.FullName));
        });
    });


    function getData(searchKey) {
        var count = 0;
        var list = [];
        for (var i = 0; i < patients.length; i++) {
            if (count > 30) {
                break;
            }
            if (searchKey == '' || patients[i].FullName.indexOf(searchKey) >= 0) {
                list.push(patients[i].FullName);
                count++;
            }
        }
        return list;
    }
</script>

