@{
    var allPatient = TempData["allPatients"] as List<PatientDto>;
    var allLabTestType = TempData["allLabTestTypes"] as List<LabTestType>;
    foreach (var item in allLabTestType)
    {
        foreach (var subitem in item.LabTestDetails)
        {
            if (String.IsNullOrEmpty(subitem.Unit))
            {
                subitem.Unit = "";
            }
            else
            {
                subitem.Unit = "(" + subitem.Unit.Trim() + ")";
            }
        }
    }
    var huyethocLabTest = allLabTestType.SingleOrDefault(x => x.LabTestTypeId == 1).LabTestDetails.ToList();
    var nuoctieuLabTest = allLabTestType.SingleOrDefault(x => x.LabTestTypeId == 2).LabTestDetails.ToList();
    var sinhHoaLabTest = allLabTestType.SingleOrDefault(x => x.LabTestTypeId == 3).LabTestDetails.ToList();
    string patientIdRef = null;
    if (TempData["patientIdRef"] != null)
    {
        patientIdRef = TempData["patientIdRef"].ToString();
    }
    else
    {
        patientIdRef = "";
    }
    var currentAccount = Session[eLTMS.Models.ConstantManager.SESSION_ACCOUNT] as Account;
}
